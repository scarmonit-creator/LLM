{
  "name": "llm",
  "version": "2.1.0-ultra-optimized",
  "description": "LLM integration project with Ultra-Advanced Optimization Systems - BREAKTHROUGH PERFORMANCE + AI-DRIVEN OPTIMIZATION",
  "main": "server-optimized.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "start:optimized": "node server-optimized.js",
    "start:ultra": "node server-ultra-optimized.js",
    "start:production": "NODE_ENV=production node server-optimized.js",
    "dev": "NODE_ENV=development nodemon server.js",
    "test": "cross-env NODE_ENV=test jest",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "build": "tsc",
    "build:tools": "tsc -p tools/tsconfig.json",
    "build:optimized": "cross-env NODE_ENV=production tsc",
    "build:ultra": "cross-env NODE_ENV=production OPTIMIZATION_LEVEL=ultra tsc",
    "build:website": "node scripts/build-website.js",
    "ultra:optimize": "node scripts/ultra-optimization-nexus.js",
    "breakthrough:optimize": "node scripts/breakthrough-system-optimizer.js",
    "system:optimize": "node scripts/complete-system-optimization.js",
    "performance:analyze": "node scripts/performance-optimizer.js",
    "health:check": "node scripts/health-check.js",
    "validate:ultra": "node scripts/validate-optimization.js",
    "concurrent:optimize": "node scripts/concurrent-optimization.js",
    "test:performance": "cross-env NODE_ENV=test node scripts/performance-tests.js",
    "optimize": "node scripts/optimization-suite.js",
    "optimize:full": "node scripts/full-optimization.js",
    "deps:check": "npm audit && npm outdated",
    "security:audit": "npm audit --audit-level=moderate",
    "production:full": "cross-env NODE_ENV=production node server-ultra-optimized.js"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0",
    "python": ">=3.8.0"
  },
  "keywords": [
    "llm",
    "ai",
    "optimization",
    "performance",
    "claude",
    "ollama",
    "jules",
    "rag",
    "websocket",
    "autonomous"
  ],
  "author": "scarmonit-creator",
  "license": "ISC",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@modelcontextprotocol/sdk": "^1.0.4",
    "@nitric/sdk": "^1.4.2",
    "better-sqlite3": "^11.8.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.1",
    "fs-extra": "^11.2.0",
    "helmet": "^8.0.0",
    "lru-cache": "^11.0.2",
    "pg": "^8.11.3",
    "winston": "^3.17.0",
    "ws": "^8.16.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/better-sqlite3": "^7.6.12",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.13.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "optionalDependencies": {
    "psutil": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Scarmonit/LLM.git"
  },
  "bugs": {
    "url": "https://github.com/Scarmonit/LLM/issues"
  },
  "homepage": "https://github.com/Scarmonit/LLM#readme",
  "nitric": {},
  "optimization": {}
}