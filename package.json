{
  "name": "llm",
  "version": "2.1.0-phase2",
  "description": "LLM integration project with Claude, A2A, and browser history access - Phase 2 Optimized",
  "main": "src/ai-bridge.js",
  "type": "module",
  "scripts": {
    "start": "node src/ai-bridge.js",
    "start:server": "node server.js",
    "start:bridge": "node src/ai-bridge.js",
    "start:enhanced": "node --expose-gc src/ai-bridge-enhanced.js",
    "start:production": "NODE_ENV=production node --expose-gc --max-old-space-size=2048 src/ai-bridge-enhanced.js",
    "start:orchestrator": "tsx orchestrator.ts",
    "start:a2a": "tsx src/a2a-agent-server.ts",
    "chat": "node src/chat-launcher.js",
    "bridge:server": "node src/ai-bridge.js",
    "bridge:client": "node src/ai-bridge-client.js",
    "bridge:optimized": "node src/ai-bridge-optimized.js",
    "bridge:enhanced": "node --expose-gc src/ai-bridge-enhanced.js",
    "bridge:selection-wiring": "node src/wiring/selection-wiring.js",
    "agents:coordinator": "node src/agents/agent-coordinator.js",
    "optimize:text-selection": "node scripts/optimize-text-selection.js",
    "optimize:current-tab": "node scripts/optimize-text-selection.js",
    "performance:analyze": "node scripts/optimize-text-selection.js",
    "performance:benchmark": "node --expose-gc scripts/performance-benchmark-suite.js",
    "performance:monitor": "node scripts/performance-monitor.js full",
    "performance:compare": "node scripts/performance-benchmark-suite.js && npm run performance:monitor",
    "cloud-sql:setup": "node scripts/cloud-sql-setup.js",
    "cloud-sql:persist": "node scripts/cloud-sql-setup.js",
    "memory:test": "node --expose-gc --max-old-space-size=512 scripts/memory-stress-test.js",
    "workers:test": "node scripts/worker-pool-test.js",
    "gc:test": "node --expose-gc scripts/gc-optimization-test.js",
    "test:bridge": "cross-env NODE_OPTIONS=--experimental-vm-modules node --test tests/ai-bridge.test.js",
    "test:enhanced": "cross-env NODE_OPTIONS=\"--experimental-vm-modules --expose-gc\" node --test tests/ai-bridge-enhanced.test.js",
    "test:memory": "cross-env NODE_OPTIONS=\"--experimental-vm-modules --expose-gc\" node --test tests/memory-optimizations.test.js",
    "test:workers": "cross-env NODE_OPTIONS=--experimental-vm-modules node --test tests/worker-pool.test.js",
    "pretest": "npm run build --silent",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules node --test tests || echo 'No tests found'",
    "pretest:watch": "npm run build --silent",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules node --test --watch tests",
    "pretest:e2e": "npm run build --silent",
    "test:e2e": "cross-env NODE_OPTIONS=--experimental-vm-modules node --test tests/claude-client.test.js tests/jules-client.test.js tests/ollama-demo.test.js",
    "test:selection": "cross-env NODE_OPTIONS=--experimental-vm-modules node tests/selection/selection-manager.test.js",
    "test:agents": "cross-env NODE_OPTIONS=--experimental-vm-modules node tests/agents/agent-coordinator.test.js",
    "test:orchestrator": "cross-env NODE_OPTIONS=\"--experimental-vm-modules --loader=tsx\" node --test tests/orchestrator.test.js",
    "test:a2a": "tsx --test tests/a2a-agent-server.test.ts",
    "test:history": "tsx --test tests/browser-history.test.js",
    "pretest:full": "npm run build --silent",
    "test:full": "cross-env NODE_OPTIONS=\"--experimental-vm-modules --loader=tsx\" node --test tests || echo 'No tests found'",
    "test:phase2": "npm run test:enhanced && npm run test:memory && npm run test:workers",
    "pretest:all": "npm run build --silent",
    "test:all": "npm run test && npm run test:orchestrator && npm run test:a2a && npm run test:phase2",
    "test:performance": "cross-env NODE_OPTIONS=\"--experimental-vm-modules --expose-gc\" node scripts/performance-test.js",
    "test:coverage": "c8 npm test",
    "test:smoke": "cross-env NODE_OPTIONS=--experimental-vm-modules node scripts/smoke-test.js",
    "create-mock-performance-results": "node scripts/create-mock-performance-results.js",
    "coverage": "c8 npm test",
    "coverage:e2e": "c8 npm run test:e2e",
    "coverage:full": "c8 npm run test:full",
    "coverage:all": "c8 npm run test:all",
    "coverage:phase2": "c8 npm run test:phase2",
    "build": "tsc",
    "bundle:chat": "esbuild src/chat-launcher.js --bundle --platform=node --format=cjs --outfile=build/chat-launcher.cjs",
    "bundle:enhanced": "esbuild src/ai-bridge-enhanced.js --bundle --platform=node --format=cjs --outfile=build/ai-bridge-enhanced.cjs --external:worker_threads",
    "build:chat-exe": "npm run build --silent && npm run bundle:chat --silent && pkg build/chat-launcher.cjs --compress GZip --targets node18-win-x64 --output release/LLMChat.exe",
    "build:production": "npm run build && npm run bundle:enhanced",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/ tests/ tools/ scripts/ --fix",
    "format": "prettier --write \"src/**/*.{js,ts}\" \"tests/**/*.{js,ts}\" \"tools/**/*.{js,ts}\" \"scripts/**/*.{js,ts}\" \"*.{js,ts,json}\"",
    "autofix": "npm run lint && npm run format",
    "verify": "npm run build && npm run test:full",
    "verify:e2e": "npm run build && npm run test:e2e",
    "verify:all": "npm run build && npm run test:all",
    "verify:phase2": "npm run performance:benchmark && npm run test:phase2",
    "deploy:benchmark": "npm run performance:benchmark && npm run verify:phase2",
    "phase2:validate": "npm run performance:benchmark && npm run memory:test && npm run workers:test"
  },
  "keywords": [
    "llm",
    "claude",
    "anthropic",
    "ai",
    "agent",
    "a2a",
    "browser-history",
    "automation",
    "optimization",
    "performance",
    "cloud-sql",
    "persistence",
    "memory-optimization",
    "worker-threads",
    "gc-optimization",
    "object-pooling",
    "phase-2"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@modelcontextprotocol/sdk": "^1.0.4",
    "axios": "^1.7.9",
    "chromadb": "^1.9.2",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "gpt-tokenizer": "^2.6.1",
    "mysql2": "^3.11.3",
    "nodemailer": "^7.0.7",
    "ws": "^8.18.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@jest/globals": "^29.7.0",
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.2",
    "@types/nodemailer": "^6.4.14",
    "@types/supertest": "^6.0.2",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "@yao-pkg/pkg": "^6.8.0",
    "c8": "^10.1.3",
    "cross-env": "^10.1.0",
    "esbuild": "^0.25.10",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "supertest": "^7.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^3.2.4"
  },
  "pkg": {
    "scripts": [
      "build/chat-launcher.cjs",
      "build/ai-bridge-enhanced.cjs"
    ],
    "assets": [
      "dist/**/*",
      "src/workers/task-worker.js"
    ]
  },
  "c8": {
    "reporter": [
      "text",
      "lcov",
      "html"
    ],
    "exclude": [
      "tests/**",
      "coverage/**",
      "node_modules/**",
      "scripts/performance-benchmark-suite.js"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "config": {
    "phase2": {
      "memoryOptimization": true,
      "workerThreads": true,
      "gcOptimization": true,
      "objectPooling": true,
      "performanceMonitoring": true
    }
  }
}