{
  "name": "llm-automation",
  "version": "1.0.0",
  "description": "LLM automation platform with concurrent optimization",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "start:enhanced": "node --expose-gc server-enhanced.js",
    "start:ultra": "node --max-old-space-size=8192 --optimize-for-size scripts/autonomous-system-validator.js && npm start",
    "build": "tsc",
    "build:tools": "node scripts/build-tools.js || echo 'Build tools script not found'",
    "concurrent:optimize": "node src/concurrent/optimizer.js || echo 'Optimizer not found yet'",
    "concurrent:full": "node src/concurrent/full-optimization.js || echo 'Full optimization not found yet'",
    "concurrent:breakthrough": "node src/concurrent/breakthrough-optimizer.js || echo 'Breakthrough optimizer not found yet'",
    "performance:concurrent": "curl -s http://localhost:8080/metrics/concurrent | jq .",
    "cloud-sql:setup": "node scripts/cloud-sql-setup.js",
    "cloud-sql:persist": "node scripts/cloud-sql-setup.js",
    "test:bridge": "cross-env NODE_OPTIONS=--experimental-vm-modules node --test tests/ai-bridge.test.js",
    "pretest": "npm run build --silent",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules node --test tests || echo 'No tests found'",
    "pretest:watch": "npm run build --silent",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules node --test --watch tests",
    "test:jest": "jest",
    "test:coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules node --test --experimental-test-coverage tests",
    "test:performance": "npm run performance:concurrent && npm run test:coverage",
    "test:concurrent": "npm run concurrent:optimize && npm run test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "validate": "npm run lint && npm run test",
    "validate:full": "npm run concurrent:optimize && npm run validate",
    "validate:ultra": "npm run concurrent:breakthrough && npm run validate:full",
    "validate:system": "node scripts/autonomous-system-validator.js",
    "optimize:ultra": "node src/ultra-performance/advanced-memory-pool.js",
    "security:scan": "node extensions/security/enhanced-security-manager.ts",
    "db:optimize": "node src/database/predictive-connection-pool.js",
    "deploy:production": "bash scripts/deploy-production-system.sh",
    "mcp:server": "node src/mcp-server/server.ts",
    "dev": "npm run build:tools && node --watch server.js",
    "dev:concurrent": "npm run concurrent:optimize && node --watch --expose-gc server-enhanced.js",
    "dev:production": "npm run concurrent:full && npm run dev:concurrent",
    "dev:ultra": "npm run concurrent:breakthrough && node --watch --expose-gc --max-old-space-size=8192 server-enhanced.js"
  },
  "keywords": [
    "llm",
    "claude",
    "automation",
    "browser-history",
    "a2a",
    "concurrent"
  ],
  "author": "scarmonit",
  "license": "ISC",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.33.1",
    "@google-cloud/secret-manager": "^5.6.0",
    "@google-cloud/sql": "^0.1.2",
    "@google-cloud/storage": "^7.16.0",
    "@modelcontextprotocol/sdk": "^0.5.0",
    "axios": "^1.7.9",
    "body-parser": "^1.20.3",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.0",
    "node-fetch": "^3.3.2",
    "pdf-parse": "^1.1.1",
    "playwright": "^1.49.1",
    "puppeteer": "^23.11.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.5",
    "cross-env": "^7.0.3",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
