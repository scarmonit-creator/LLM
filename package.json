    "type": "ultra-performance",
    "features": {
      "autonomous_optimization": true,
      "real_time_monitoring": true,
      "clustering": true,
      "advanced_caching": true,
      "memory_management": true,
      "garbage_collection": true,
      "performance_analytics": true,
      "auto_scaling": true
    },
    "performance_targets": {
      "response_time": "<25ms",
      "memory_efficiency": ">95%",
      "cache_hit_rate": ">80%",
      "availability": "99.99%",
      "throughput": ">5000 RPS",
      "error_rate": "<0.1%"
    }
  },
  "nitric": {
    "version": "2.1.0",
    "framework": "cloud-native",
    "features": {
      "serverless": true,
