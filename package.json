{
  "name": "llm-application",
  "version": "1.0.0",
  "description": "LLM application with Anthropic Claude integration and browser history automation",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "jules": "node src/jules-demo.js",
    "ollama": "node src/ollama-demo.js",
    "multiagent": "tsx orchestrator.ts",
    "build": "tsc",
    "build:ts": "tsc",
    "test": "node --test tests/*.test.js",
    "test:full": "npm run lint && npm run format && npm test",
    "lint": "eslint src/ tests/ --fix",
    "format": "prettier --write \"src/**/*.{js,ts}\" \"tests/**/*.{js,ts}\" \"*.{js,ts,json}\"",
    "autofix": "npm run lint && npm run format",
    "verify": "npm run build && npm run test:full"
  },
  "keywords": [
    "llm",
    "claude",
    "anthropic",
    "ai",
    "browser-history",
    "automation",
    "autonomous-agent"
  ],
  "author": "Parker Dunn <scarmonit>",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.0",
    "axios": "^1.12.2",
    "dotenv": "^16.4.5",
    "node-browser-history": "^2.3.0",
    "@langchain/core": "^0.3.0",
    "@langchain/community": "^0.3.0",
    "langchain": "^0.3.0",
    "commander": "^12.0.0",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^24.7.2",
    "eslint": "^9.15.0",
    "prettier": "^3.4.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
