# LLM Framework Environment Configuration
# Production-safe defaults - providers disabled until keys are added

# Server Configuration
PORT=8080
NODE_ENV=production
OPTIMIZATION_LEVEL=ultra

# Performance Optimization Settings
GLOBAL_TIMEOUT_MS=60000
LOG_LEVEL=info
COMPRESSION_THRESHOLD=1024
MAX_CONNECTIONS=100
HEALTH_CHECK_INTERVAL=10000
RATE_LIMIT_TOKENS=10
TARGET_EFFICIENCY=85

# Provider API Keys (add your keys to enable providers)
# ANTHROPIC_API_KEY=
# JULES_API_KEY=
# OPENAI_API_KEY=
# PERPLEXITY_API_KEY=

# RAG Configuration (optional)
# CHROMA_DB_PATH=./data/chroma
# VECTOR_STORE_URL=

# Database Configuration (optional)
# DATABASE_URL=
# POSTGRES_URL=

# Monitoring and Analytics (optional)
# SENTRY_DSN=
# ANALYTICS_ENABLED=false

# Security Settings
CORS_ORIGIN=*
RATE_LIMIT_ENABLED=true
HELMET_ENABLED=true

# Browser History Settings
BROWSER_HISTORY_ENABLED=true
BROWSER_HISTORY_PATH=./data/browser
BROWSER_HISTORY_AUTO_SYNC=true

# Deployment Settings
FLY_APP_NAME=llm-framework
RENDER_SERVICE_NAME=llm-framework
RAILWAY_SERVICE_NAME=llm-framework

# Feature Flags
A2A_AGENT_ENABLED=true
WEBSOCKET_ENABLED=true
PERFORMANCE_MONITORING=true
AUTO_OPTIMIZATION=true
HEALTH_CHECKS=true

# Development Settings (override in .env.local)
# DEBUG=*
# VERBOSE_LOGGING=false