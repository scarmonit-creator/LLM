{
  "manifest_version": 3,
  "name": "Ultra Text Analyzer - AI-Powered Analysis",
  "version": "3.0.0",
  "description": "Ultra-fast AI-powered text analysis with sentiment detection, readability scoring, keyword extraction, and real-time performance monitoring. Enhanced with beautiful UI and advanced caching.",
  
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "contextMenus",
    "notifications"
  ],
  
  "host_permissions": [
    "https://github.com/*",
    "https://console.cloud.google.com/*",
    "https://firebase.google.com/*",
    "https://*.googleapis.com/*",
    "https://docs.google.com/*",
    "https://medium.com/*",
    "https://stackoverflow.com/*",
    "https://reddit.com/*",
    "https://news.ycombinator.com/*"
  ],
  
  "background": {
    "service_worker": "optimized-background.js",
    "type": "module"
  },
  
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*",
        "https://console.cloud.google.com/*",
        "https://firebase.google.com/*",
        "https://*.googleapis.com/*",
        "https://docs.google.com/*",
        "https://medium.com/*",
        "https://stackoverflow.com/*",
        "https://reddit.com/*",
        "https://news.ycombinator.com/*"
      ],
      "js": [
        "optimized-text-analyzer.js",
        "ultra-optimized-content.js"
      ],
      "run_at": "document_idle"
    }
  ],
  
  "action": {
    "default_popup": "ultra-optimized-popup.html",
    "default_title": "Ultra Text Analyzer",
    "default_icon": {
      "16": "data:image/svg+xml;base64,",
      "32": "data:image/svg+xml;base64,"
    }
  },
  
  "commands": {
    "analyze-selection": {
      "suggested_key": {
        "default": "Ctrl+Shift+A",
        "mac": "Command+Shift+A"
      },
      "description": "Analyze selected text with AI"
    },
    "analyze-page": {
      "suggested_key": {
        "default": "Ctrl+Shift+P",
        "mac": "Command+Shift+P"
      },
      "description": "Analyze entire page content"
    },
    "quick-analysis": {
      "suggested_key": {
        "default": "Ctrl+Shift+Q",
        "mac": "Command+Shift+Q"
      },
      "description": "Quick analysis with floating display"
    }
  },
  
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'; base-uri 'self';"
  },
  
  "web_accessible_resources": [
    {
      "resources": [
        "optimized-text-analyzer.js",
        "ultra-optimized-content.js",
        "utils/*.js",
        "extractors/*.js"
      ],
      "matches": [
        "https://github.com/*",
        "https://console.cloud.google.com/*",
        "https://firebase.google.com/*",
        "https://*.googleapis.com/*",
        "https://docs.google.com/*",
        "https://medium.com/*",
        "https://stackoverflow.com/*",
        "https://reddit.com/*",
        "https://news.ycombinator.com/*"
      ]
    }
  ],
  
  "icons": {
    "16": "data:image/svg+xml;base64,",
    "32": "data:image/svg+xml;base64,",
    "48": "data:image/svg+xml;base64,",
    "128": "data:image/svg+xml;base64,"
  },
  
  "minimum_chrome_version": "120",
  
  "homepage_url": "https://github.com/scarmonit-creator/LLM",
  
  "author": "scarmonit-creator",
  
  "short_name": "UltraTextAI"
}