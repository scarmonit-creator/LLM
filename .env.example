# Anthropic API Key (for Claude)
ANTHROPIC_API_KEY=your_api_key_here
MODEL=claude-3-5-sonnet-20241022
MAX_TOKENS=4096
TEMPERATURE=1.0

# Google Gemini API Key
# Get your key from: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-1.5-pro

# Jules API Key
# Get your key from: https://jules.app (Settings > API Keys)
JULES_API_KEY=your_jules_api_key_here

# Ollama Configuration
# For local Ollama: set OLLAMA_API_BASE to your local server
OLLAMA_API_BASE=http://localhost:11434
# For Ollama Cloud: get your key from https://ollama.com/cloud
OLLAMA_API_KEY=your_ollama_cloud_api_key_here

# AI Bridge Configuration (for multi-agent coordination)
AI_BRIDGE_PORT=4567
AI_BRIDGE_HTTP_PORT=4568
AI_BRIDGE_URL=ws://localhost:4567
AGENT_ID=agent-dev
AGENT_ROLE=coder

# Performance Configuration (optional)
GLOBAL_TIMEOUT_MS=60000
LOG_LEVEL=info
COMPRESSION_THRESHOLD=1024
MAX_CONNECTIONS=100
HEALTH_CHECK_INTERVAL=10000
RATE_LIMIT_TOKENS=10
OPTIMIZATION_LEVEL=ultra
TARGET_EFFICIENCY=85